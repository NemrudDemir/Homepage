{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": ".logs\\log.json",
          "rollingInterval": "Day"
        }
      },
      { "Name": "Console" }
    ]
  }
}
