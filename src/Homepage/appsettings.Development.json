{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": ".logs\\log.json",
          "rollingInterval": "Day"
        }
      },
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*",
  "MailSettings": {
    "MailAddress": "sender@domain.com",
    "Password": "mailPassword",
    "Host": "smtp.example.com",
    "Port": 587,
    "ToMailAddress": "receiver@mail.com",
    "EnableSsl": true
  },
  "BuildProperties": {
    "BuildId": "123456"
  }
}
